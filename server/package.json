{
  "name": "qd-server",
  "version": "1.0.0",
  "description": "A quick and dirty server",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon dist/server.js\" \"npm run copy-assets\"",
    "build": "rimraf dist && npx tsc",
    "copy-assets": "cp -R ./src/templates/ ./dist/templates"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "html-pdf": "^3.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  }
}
